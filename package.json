{
    "name": "io.cordova.hellocordova",
    "version": "1.0.0",
    "displayName": "HelloCordova",
    "cordova": {
        "platforms": [
            "android",
            "ios"
        ],
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-camera": {
                "CAMERA_USAGE_DESCRIPTION": "Taskworld would like to access your camera for attachment purpose only",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": "Taskworld would like to access your photo library for attachment purpose only"
            },
            "cordova-plugin-code-push": {},
            "cordova-plugin-crosswalk-webview": {
                "XWALK_VERSION": "23+",
                "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
                "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
                "XWALK_MODE": "embedded",
                "XWALK_MULTIPLEAPK": "true"
            },
            "cordova-plugin-ios-no-export-compliance": {},
            "cordova-plugin-keyboard": {},
            "cordova-plugin-media-capture": {
                "CAMERA_USAGE_DESCRIPTION": " ",
                "MICROPHONE_USAGE_DESCRIPTION": " ",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": " "
            },
            "cordova-plugin-screen-orientation": {},
            "cordova-plugin-settings-hook": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-wkwebview-engine": {},
            "phonegap-plugin-push": {
                "SENDER_ID": "721967826372"
            }
        }
    },
    "dependencies": {
        "code-push": "^1.8.0-beta",
        "cordova-android": "^6.2.1",
        "cordova-ios": "~4.3.0",
        "cordova-plugin-camera": "^2.3.1",
        "cordova-plugin-code-push": "^1.9.2-beta",
        "cordova-plugin-compat": "^1.1.0",
        "cordova-plugin-crosswalk-webview": "^2.3.0",
        "cordova-plugin-device": "^1.1.6",
        "cordova-plugin-dialogs": "^1.3.3",
        "cordova-plugin-file": "^4.3.3",
        "cordova-plugin-file-transfer": "^1.6.3",
        "cordova-plugin-ios-no-export-compliance": "https://github.com/mikaoelitiana/cordova-plugin-ios-no-export-compliance.git",
        "cordova-plugin-keyboard": "^1.1.5",
        "cordova-plugin-media-capture": "^1.4.3",
        "cordova-plugin-screen-orientation": "^1.4.3",
        "cordova-plugin-settings-hook": "^0.2.5",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2",
        "cordova-plugin-wkwebview-engine": "^1.1.3",
        "cordova-plugin-zip": "^3.1.0",
        "phonegap-plugin-push": "^1.9.4"
    }
}